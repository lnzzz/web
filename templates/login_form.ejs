<div id="log">
    <div class="container">
        <div class="d-flex justify-content-center">
            <a class="navbar-brand" href="#">
                <img src="/images/EDS.svg" class="main_logo"/>
            </a>
        </div>
        <div class="row">
            <div class="form-signin">
                <form action="/login" method="POST" autocomplete="off">
                    <h1 class="text-uppercase hv">Login</h1>
                    <div class="form-floating">
                        <input type="text" class="form-control" id="floatingInput" name="username" autocomplete="off" placeholder="Usuario">
                        <label for="floatingInput">Usuario</label>
                    </div>
                    <div class="form-floating mt-4">
                        <input type="password" class="form-control rounded" id="floatingPassword" name="password" placeholder="Password">
                        <label for="floatingPassword">Password</label>
                    </div>

                    <div class="form-check text-start my-3">
                        <input class="form-check-input" type="checkbox" name="rememberMe" id="flexCheckDefault">
                        <label class="form-check-label text-white" for="flexCheckDefault">
                        Recordarme
                        </label>
                    </div>
                    <button class="btn btn-primary w-50 py-2 violet-bg violet-border mt-4" type="submit">LOGIN</button>
                </form>
            </div>
        </div>
    </div>
</div>